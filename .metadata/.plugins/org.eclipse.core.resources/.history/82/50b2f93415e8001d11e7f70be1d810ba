import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int side = sc.nextInt();

		char alpha = 'A';

		for (int i = 0; i < side; i++) {
			for (int j = 0; j < side; j++) {
				System.out.print(side);
				alpha++;
			}	
			System.out.println();
		}
	}
}