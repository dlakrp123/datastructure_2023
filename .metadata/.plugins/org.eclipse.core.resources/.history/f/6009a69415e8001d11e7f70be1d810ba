import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int side = sc.nextInt();

		char alpha = 'A';

		for (int i = side; i > 0 ; i--) {
			for (int j = side; j > 0; j--) {
				System.out.print(alpha);
				System.out.println();
				alpha++;
			}	
			System.out.println();
		}
	}
}