import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int side = sc.nextInt();

		char alpha = 'A';

		
		for (int i = 0; i < side; i++) {
			for (int j = side; j > 0; j--) {
				System.out.print(alpha + " ");
				alpha++;
			}
			System.out.println();
		}
	}
}