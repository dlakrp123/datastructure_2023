
public class ArrayTest_4 {
	
	static void InsertArray(int[]a,int r,int n) {
		int k = a.length-r-1;
		
		for(int i = k ; i<a.length ; i++) {
			a[i] = a[i+1];
		}
		
		for(int i = 0;  i < a.length ; i ++)
			System.out.print(a[i]);
	}
	
	public static void main(String[] args) {
		int arr[] = {6,3,1,9,7,2,4};
		
		InsertArray(arr, 3, 5);
	}
}
