import java.util.Scanner;

public class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int side = sc.nextInt();

		char alpha = 'A';

		for (int i = side; i > 0 ; i--) {
			for (int j = 0; j < side; j++) {
				System.out.print(alpha + " ");
				System.out.println();
				alpha++;
			}	
		}
	}
}