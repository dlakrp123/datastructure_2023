public class Solution {
    public static void main(String[] args) {
        int n = 5; // 총 행 수
        int m = 5; // 총 열 수

        char[][] matrix = new char[n][m];
        char ch = 'A';

        for (int i = n - 1; i >= 0; i--) {
            if (i % 2 == n % 2) {
                for (int j = m - 1; j >= 0; j--) {
                    matrix[i][j] = ch++;
                    if (ch > 'Z') {
                        ch = 'A';
                    }
                }
            } else {
                for (int j = 0; j < m; j++) {
                    matrix[i][j] = ch++;
                    if (ch > 'Z') {
                        ch = 'A';
                    }
                }
            }
        }

        // 결과 출력
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++) {
                System.out.print(matrix[i][j] + " ");
            }
            System.out.println();
        }
    }
}
